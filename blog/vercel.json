{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "python3.11",
      "maxDuration": 15,
      "memory": 1024
    }
  },
  "env": {
    "DJANGO_SETTINGS_MODULE": "config.settings",
    "PYTHONUNBUFFERED": "1"
  },
  "routes": [
    {
      "src": "^/static/(.*)$",
      "dest": "/static/$1",
      "headers": { "cache-control": "public, max-age=31536000, immutable" }
    },
    {
      "src": "^/media/(.*)$",
      "dest": "/media/$1"
    },
    {
      "src": "^(?!/static/|/media/).*",
      "dest": "api/index.py"
    }
  ]
}